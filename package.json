{
  "name": "js-dev-environment",
  "version": "1.0.0",
  "description": "Boilerplate for JS projects",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start":"npm-run-all --parallel security-check open:src test:watch start-mockapi",
    "open:src":"babel-node buildScripts/server.js",
    "security-check":"nsp check",
    "share":"npm-run-all --parallel localtunel opne:src",
    "localtunnel": "lt --port 3000",
    "test":"mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch":"npm run test -- --watch",
    "generate-mock-data":"babel-node buildScripts/generateMockData.js",
    "prestart-mockapi":"npm run generate-mock-data",
    "start-mockapi":"json-server --watch src/api/db.json --port 3001"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SACHIN5SOS/js-dev-environment.git"
  },
  "author": "Sachin Bhandari",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SACHIN5SOS/js-dev-environment/issues"
  },
  "homepage": "https://github.com/SACHIN5SOS/js-dev-environment#readme",
  "dependencies": {
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "cheerio": "^1.0.0-rc.2",
    "compression": "^1.7.2",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-watch": "^3.1.5",
    "express": "^4.17.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "jsdom": "^11.11.0",
    "json-schema-faker": "^0.5.0-rc15",
    "json-server": "^0.14.0",
    "localtunnel": "^1.9.0",
    "mocha": "^5.2.0",
    "nock": "^9.3.3",
    "npm-run-all": "^4.1.3",
    "nsp": "^3.2.1",
    "numeral": "^2.0.6",
    "open": "0.0.5",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "webpack": "^4.12.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-md5-hash": "0.0.6"
  }
}
